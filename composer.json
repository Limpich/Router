{
  "name": "limpich/router",
  "description": "Annotation router",
  "minimum-stability": "dev",
  "license": "proprietary",
  "authors": [
    {
      "name": "Andrey Smirnov",
      "email": "a.asmirnov2002@gmail.com"
    }
